    "hardhat-deploy": "0.11.45",
    "hardhat-gas-reporter": "1.0.10",
    "hardhat-tracer": "2.7.0",
    "rimraf": "6.0.1",
    "solc": "0.8.28",
    "solhint": "5.0.3",
    "solidity-coverage": "0.8.13",
    "solidity-docgen": "0.6.0-beta.36",
    "zksync-ethers": "6.15.0"
