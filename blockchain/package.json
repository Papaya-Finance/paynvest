  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync": "^1.1.0",
    "@matterlabs/hardhat-zksync-deploy": "1.6.0",
    "@matterlabs/hardhat-zksync-solc": "1.2.5",
    "@matterlabs/hardhat-zksync-verify": "1.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.8",
