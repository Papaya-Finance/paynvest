{
  "name": "@papaya-finance/paynvest",
  "version": "0.0.1",
  "description": "Papaya blockchain smart-contracts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Papaya-Finance/paynvest.git"
  },
  "license": "BUSL-1.1",
  "files": [
    "contracts/*.sol",
    "contracts/helpers",
